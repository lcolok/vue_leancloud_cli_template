{
  "name": "vue_leancloud_cli_template",
  "version": "0.0.1",
  "private": true,
  "description": "A sample Node.js app using Express,with powerful Vue CLI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js -- ",
    "serve": "vue-cli-service serve",
